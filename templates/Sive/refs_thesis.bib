@ARTICLE{Ben-Moshe2001,
  author = {Ben-Moshe, A. and Dayan, T. and Simberloff, D.},
  title = {Convergence in morphological patterns and community organization
	between {O}ld and {N}ew {W}orld rodent guilds},
